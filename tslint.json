{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "curly": true,
    "indent": [true, "spaces", 2],
    "no-console": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "single", "jsx-double", "avoid-escape"],
    "space-within-parens": [true, 0],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator"
    ]
  },
  "rulesDirectory": []
}
