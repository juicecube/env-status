{
  "name": "env-status",
  "version": "0.22.0",
  "description": "A command to show each env status, whether it is using for testing or available for using.",
  "keywords": [
    "env",
    "status"
  ],
  "homepage": "https://github.com/webyom/env-status",
  "author": {
    "name": "Gary Wang",
    "email": "webyom@gmail.com",
    "url": "http://webyom.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webyom/env-status.git"
  },
  "bugs": {
    "url": "https://github.com/webyom/env-status/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/webyom/env-status/blob/master/LICENSE-MIT"
    }
  ],
  "engines": {
    "node": ">= 8.1.2"
  },
  "main": "index.js",
  "bin": {
    "arc-diff": "./lib/bin/arc-diff.js",
    "arc-land": "./lib/bin/arc-land.js",
    "env-status": "./lib/bin/env-status.js",
    "env-status-pre-push-hook": "./lib/bin/env-status-pre-push-hook.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "eslint": "npx eslint *.js bin/*",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run test",
      "post-commit": "./bin/env-status --gen",
      "pre-push": "./bin/env-status-pre-push-hook"
    }
  },
  "dependencies": {
    "as-table": "^1.0.52",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "fetch": "^1.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-eslint": "^8.2.6",
    "codecov": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-webyom": "^1.4.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0"
  }
}
